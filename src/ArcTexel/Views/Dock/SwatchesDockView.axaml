<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:palettes="clr-namespace:ArcTexel.Views.Palettes"
             xmlns:xaml="clr-namespace:ArcTexel.Models.Commands.XAML"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="ArcTexel.Views.Dock.SwatchesDockView">
    <palettes:SwatchesView
        SelectSwatchCommand="{xaml:Command ArcTexel.Colors.SelectColor, UseProvided=True}"
        Swatches="{Binding DocumentManagerSubViewModel.ActiveDocument.Swatches}" />
</UserControl>
